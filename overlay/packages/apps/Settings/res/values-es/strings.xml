<resources xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- Introduction detail message shown in fingerprint enrollment dialog [CHAR LIMIT=NONE]-->
    <string name="security_settings_fingerprint_enroll_introduction_message" msgid="9193978734907066950">"Colocar y mover ligeramente la punta del dedo sobre la tecla Home para desbloquear el teléfono, autorizar compras o acceder a aplicaciones. Tenga cuidado de que huellas digitales agrega - cualquier huella digital añadida será capaz de hacer estas cosas."</string>

    <!-- Message shown in fingerprint enrollment dialog to begin enrollment [CHAR LIMIT=NONE] -->
    <string name="security_settings_fingerprint_enroll_start_message" msgid="3909929328942564524">"Mueve ligeramente el dedo sobre la tecla Home"</string>

    <!-- Message shown when fingerprint enrollment is completed during setup wizard [CHAR LIMIT=NONE] -->
    <string name="setup_fingerprint_enroll_finish_message" msgid="7234264291957984004">"Mueve ligeramente el dedo sobre la tecla Home para desbloquear el dispositivo."</string>

    <!-- Text shown in fingerprint enroll when we didn't observe progress for a few seconds. [CHAR LIMIT=100] -->
    <string name="security_settings_fingerprint_enroll_lift_touch_again" msgid="1888772560642539718">"Levante el dedo, y deslícelo suavemente sobre la tecla Home otra vez"</string>

    <!-- Message shown in fingerprint enrollment dialog to locate the sensor -->
    <string name="security_settings_fingerprint_enroll_find_sensor_message" msgid="1959655161889313661">"Localiza el sensor de huellas digitales situado en la tecla Home."</string>
</resources>
